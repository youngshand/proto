{
  "name": "prototype-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run watch-js & npm run watch-sass",
    "watch-js": "nodemon ./bin/www",
    "watch-sass": "node-sass --watch public/stylesheets/main.scss public/stylesheets/main.css",
    "compile": "jade views -o build -P"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "browser-sync": "^2.11.1"
  }
}
